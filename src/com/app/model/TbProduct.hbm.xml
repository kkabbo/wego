<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.app.model.TbProduct" table="tb_product" schema="wego_db">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="typeId" column="typeId"/>
        <property name="description" column="description"/>
        <property name="normalPrice" column="normalPrice"/>
        <property name="marketPrice" column="marketPrice"/>
        <property name="images" column="images"/>
        <property name="sellCount" column="sellCount"/>
        <property name="date" column="date"/>
        <property name="content" column="content"/>
        <property name="stock" column="stock"/>
        <property name="status" column="status"/>
    </class>
</hibernate-mapping>