<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.app.model.TbAddress" table="tb_address" schema="wego_db">
        <id name="id" column="id"/>
        <property name="userId" column="userId"/>
        <property name="contacts" column="contacts"/>
        <property name="phone" column="phone"/>
        <property name="province" column="province"/>
        <property name="city" column="city"/>
        <property name="county" column="county"/>
        <property name="address" column="address"/>
        <property name="isDefault" column="isDefault"/>
    </class>
</hibernate-mapping>